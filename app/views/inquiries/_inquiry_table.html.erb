<table class="table table-hover table-striped text-center align-self-start">
    <thead>
        <tr class="table-primary">
            <th scope="col"><%= t('.setsumeikai_id') %></th>
            <th scope="col"><%= t('.parent_name') %></th>
            <th scope="col"><%=t('.phone') %></th>
            <th scope="col"><%= t('.email') %></th>
            <th scope="col"><%= t('.child_name') %></th>
            <th scope="col"><%= t('.child_birthday') %></th>
            <th scope="col"><%= t('.kindy') %></th>
            <th scope="col"><%= t('.ele_school') %></th>
            <th scope="col"><%= t('.start_date') %></th>
            <th scope="col"><%= t('.referrer') %></th>
            <th scope="col"><%= t('.created_at') %></th>
            <th scope="col"><%= t('.requests') %></th>
            <th scope="col"><%= t('.notes') %></th>
            <th scope="col"><%= t('.edit') %></th>
            <th scope="col"><%= t('.delete') %></th>
        </tr>
        <tr>
            <th data-controller="filter"
                data-filter-siblings-value="0"
                data-filter-col-value=".setsumeikai_id">
                <div class="form-floating">
                    <%= text_field_tag 'setsumeikai_id_filter', nil, { placeholder: t('.filter'), class: 'form-control', data: { action: 'keydown.enter->filter#change', 'filter-target' => 'input' } }%>
