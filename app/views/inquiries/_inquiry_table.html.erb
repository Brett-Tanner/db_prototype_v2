                    <%= text_field_tag 'kindy_filter', nil, { placeholder: t('.filter'), class: 'form-control', data: { action: 'keydown.enter->filter#change', 'filter-target' => 'input' } }%>
                    <%= label_tag 'kindy_filter', t('.filter'), class: 'form-label' %>
                </div>
            </th>
