                </div>
            </th>
            <th data-controller="filter"
                data-filter-siblings-value="0"
                data-filter-col-value=".created_at">
                <div class="form-floating">
                    <%= text_field_tag 'created_at_filter', nil, { placeholder: t('.filter'), class: 'form-control', data: { action: 'keydown.enter->filter#change', 'filter-target' => 'input' } }%>
